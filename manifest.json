{
  "manifest_version": 2,

  "name": "Bashkortostan app",
  "description": "Bashkortostan app",
  "version": "1.1",

  "icons": {
    "128": "./icons/dich.png"
  },

  "permissions": [
    "tabs", "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "./icons/dich.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
